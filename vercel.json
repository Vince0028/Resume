{
  "version": 2,
  "buildCommand": "bash vercel-build.sh",
  "outputDirectory": "public",
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs20.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/resume",
      "dest": "/index.html?resume=1"
    },
    {
      "src": "/(.*)",
      "dest": "/terminal/index.html"
    }
  ]
}