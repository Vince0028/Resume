{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "^/(.*\\.(js|css|png|jpg|jpeg|svg|gif|ico|webp|woff2|woff|ttf|map))$", "dest": "/$1" },
    { "src": "^/_next/static/(.*)$", "dest": "/_next/static/$1" },
    { "src": "^/(.*)$", "dest": "/index.html" }
  ]
}
