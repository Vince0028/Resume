{
  "version": 2,
  "buildCommand": "cd terminal && npm install && npm run build",
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/",
      "destination": "/terminal/dist/index.html"
    },
    {
      "source": "/resume",
      "destination": "/index.html?resume=1"
    },
    {
      "source": "/terminal/(.*)",
      "destination": "/terminal/dist/$1"
    }
  ]
}